<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(google_form) -%>">
  <span class='title'>
    <%= google_form.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.google_forms_google_form_path(google_form),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_google_forms_admin_google_form_path(google_form),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.google_forms_admin_google_form_path(google_form),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => google_form.name),
        :method => :delete %>
  </span>
</li>
