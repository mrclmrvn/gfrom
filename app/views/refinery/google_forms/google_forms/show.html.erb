<% content_for :body_content_title do %>
  <%= @google_form.name %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>Name</h1>
    <p>
      <%=raw @google_form.name %>
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="google_forms">
      <% @google_forms.each do |google_form| %>
        <li>
          <%= link_to google_form.name, refinery.google_forms_google_form_path(google_form) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
